{
  "name": "@doremijs/igit-core",
  "version": "0.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "igit-core",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "description": "A CLI tool for combining git workflows with Rust bindings",
  "keywords": [
    "igit",
    "cli",
    "git",
    "hooks"
  ],
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "ava": "^6.0.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  },
  "optionalDependencies": {
    "@doremijs/igit-core-win32-x64-msvc": "0.0.1",
    "@doremijs/igit-core-darwin-x64": "0.0.1",
    "@doremijs/igit-core-linux-x64-gnu": "0.0.1",
    "@doremijs/igit-core-darwin-arm64": "0.0.1",
    "@doremijs/igit-core-linux-arm64-gnu": "0.0.1",
    "@doremijs/igit-core-linux-arm64-musl": "0.0.1",
    "@doremijs/igit-core-win32-arm64-msvc": "0.0.1",
    "@doremijs/igit-core-linux-x64-musl": "0.0.1"
  }
}
